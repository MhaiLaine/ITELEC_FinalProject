@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col s6 m6 l6">
        <table id="myTable" class="display">
            <thead>
                <tr>
                    <th>User ID</th>
                    <th>Deck ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Username</th>
                    <th>Phone Number</th>
                    <th>Email</th>
                    <th>Password</th>

                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="registrationData in userData">
                    <td>{{registrationData.uData.userID}}</td>

                    <td>{{registrationData.uData.firstName}}</td>
                    <td>{{registrationData.uData.lastName}}</td>
                    <td>{{registrationData.uData.userName}}</td>
                    <td>{{registrationData.uData.userPhone}}</td>
                </tr>
            </tbody>
        </table>
    </div>

     @*charts*@
